{
  "database": "Zotero",
  "noteImportFolder": "_zotero/imported_items",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Chicago Manual of Style 17th (author-date)",
      "format": "formatted-citation",
      "cslStyle": "chicago-author-date"
    }
  ],
  "exportFormats": [
    {
      "name": "Zotero Entry Importer (Paper/Book)",
      "outputPathTemplate": "_zotero/imported_items/@{{citekey}}.md",
      "imageOutputPathTemplate": "_zotero/imported_items/{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "_zotero/_zotero_import_template.md",
      "cslStyle": "chicago-fullnote-bibliography"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": false,
  "whichNotesToOpenAfterImport": "first-imported-note",
  "shouldShowCiteSuggest": true
}